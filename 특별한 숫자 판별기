#include <stdio.h>

void main()
{
    int num, a, b, c, sum;
    scanf("%d", &num);
    a = num / 100;
    b = (num / 10) % 10;
    c = num % 10;
    sum = a + b + c;

    if ((num % 3 == 0) && (num % 2 != 0)) {
        printf("특별한 숫자입니다.\n");
    } else if (sum % 7 == 0) {
        printf("특별한 숫자입니다.\n");
    } else if (a > c) {
        printf("특별한 숫자입니다.\n");
    } else {
        printf("특별한 숫자가 아닙니다.\n");
    }
}
