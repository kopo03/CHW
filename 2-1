#include <stdio.h>

void main()
{
    int A, B;
    scanf("%d", &A);
    scanf("%d", &B);

    if (A % B == 0) {
        printf("%d", A / B);
    } else if (A > B && A % B != 0) {
        printf("%d", A % B);
    } else if (A < B) {
        printf("%d", A + B);
    } else {
        printf("%d", A);
        printf("%d", B);
    }
}
